{
  "name": "@awesome-prague/backend-app",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "test": "jest",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "prisma-binding": "2.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/faker": "^4.1.4",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.0",
    "dotenv-cli": "1.4.0",
    "faker": "^4.1.0",
    "graphql-cli": "2.16.7",
    "jest": "^23.6.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prisma": "1.17.0",
    "rimraf": "2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "6.2.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.9.2"
  }
}
